{
    "name": "Pulse2Pulse-MITBIH-G-NL4-IC64-K4-S2-BN-D-NL3-IC64-K4-S2-SF8-SN",
    "exp_dir": "/home/bejar/bsc/misiones/Series/Models/GAN/",
    "model": {
        "generator": {
            "class": "ai4ha.GAN.Generators.Pulse2PulseGANGenerator",
            "latent_dim": [1, 256], 
            "params": {
                "seq_length": 256,
                "seq_channels": 1,
                "init_channels": 64,
                "n_layers": 4,
                "kernel_size": 4,
                "stride": 2,
                "batch_norm": true,
                "n_classes": 5
            }
  
        },
        "discriminator": {
            "class": "ai4ha.GAN.Discriminators.WaveGANDiscriminator",
            "params": {
                "input_size":256,
                "input_channels":1,
                "n_layers":3,
                "init_channels":64,
                "kernel_size":4,
                "stride":2,
                "shift_factor":8,
                "n_classes":5,
                "sp_norm":true
            }
        }
    },
    "optimizer": {
        "lr": 0.0002,
        "beta1": 0.5,
        "beta2": 0.999
    },
    "scheduler": {
        "step_size": 30,
        "gamma": 0.1
    },
    "train": {
        "batch_size": 64,
        "num_epochs": 5,
        "log_interval": 10,
        "save_interval": 10,
        "lr_warmup_steps": 10,
        "lambda_gp": 0.1,
        "checkpoint_epoch_freq":3,
        "best_model": true
    },
    "projectconf": {
        "total_limit": 2
    },
    "accelerator": {
        "gradient_accumulation_steps": 1,
        "mixed_precision": "no",
        "log_with": "wandb"
    },
    "samples": {
        "samples_freq": 25,
        "samples_num": 20,
        "samples_gen": 1000
    },
    "dataset": {
        "name": "EBHI",
        "nclasses": 5,
        "train": {
            "class": "ai4ha.data.series.MITBIHDataLoader.MITBIHtrain",
            "params": {
                "filename": "/home/bejar/ssdstorage/MITBIH/mitbih_train.csv",
                "n_samples": 2000,
                "resamp": false,
                "oneD": true,
                "fixsize": 256,
                "normalize": false
            }
        },
        "test": {
            "class": "ai4ha.data.series.MITBIHDataLoader.MITBIHtest",
            "params": {
                "filename": "/home/bejar/ssdstorage/MITBIH/mitbih_test.csv",
                "n_samples": 100,
                "resamp": false,
                "oneD": true,
                "fixsize": 256,
                "normalize": false
            }
        },
        "dataloader": {
            "batch_size": 64,
            "num_workers": 6,
            "shuffle": true
        }
    },
    "time": 12
}