{
    "name": "WaveGAN-MITBIH",
    "exp_dir": "/home/bejar/PycharmProjects/misiones/Series/Models/WveGAN/",
    "model": {
        "generator": {
            "latent_dim":128,
            "init_dim":16,
            "init_channels":256,
            "output_size":256,
            "output_channels":1,
            "kernel_size":8,
            "stride":2,
            "batch_norm":true,
            "n_classes":5
        },
        "discriminator": {
            "input_size":256,
            "input_channels":1,
            "n_layers":3,
            "init_channels":16,
            "kernel_size":4,
            "stride":2,
            "padding":1,
            "shift_factor":2,
            "n_classes":5,
            "spectral_norm":true
        }
    },
    "optimizer": {
        "lr": 0.0002,
        "beta1": 0.5,
        "beta2": 0.999
    },
    "scheduler": {
        "step_size": 30,
        "gamma": 0.1
    },
    "train": {
        "batch_size": 64,
        "num_epochs": 5,
        "log_interval": 10,
        "save_interval": 10,
        "lr_warmup_steps": 10,
        "lambda_gp": 0.1,
        "checkpoint_freq": 2000,
        "checkpoint_epoch_freq": 2
    },
    "data": {
        "path": "data/processed/"
    },
    "projectconf": {
        "total_limit": 2
    },
    "accelerator": {
        "gradient_accumulation_steps": 1,
        "mixed_precision": "no",
        "log_with": "wandb"
    },
    "samples": {
        "samples_freq": 25,
        "samples_num": 20,
        "samples_gen": 1000
    },
    "dataset": {
        "name": "EBHI",
        "nclasses": 5,
        "train": {
            "class": "ai4ha.data.series.MITBIHDataLoader.MITBIHtrain",
            "params": {
                "filename": "/home/bejar/ssdstorage/MITBIH/mitbih_train.csv",
                "n_samples": 2000,
                "resamp": false,
                "oneD": true,
                "fixsize": 256,
                "normalize": false
            }
        },
        "test": {
            "class": "ai4ha.data.series.MITBIHDataLoader.MITBIHtest",
            "params": {
                "filename": "/home/bejar/ssdstorage/MITBIH/mitbih_test.csv",
                "n_samples": 100,
                "resamp": false,
                "oneD": true,
                "fixsize": 256,
                "normalize": false
            }
        },
        "dataloader": {
            "batch_size": 16,
            "num_workers": 6,
            "shuffle": true
        }
    },
    "time": 12
}