{
    "name": "WaveGAN-MITBIH-G-LD512-ID32-IC64-K8-S2-IN-D-NL3-IC8-K4-S2-SF8-SN-GP1",
    "exp_dir": "/gpfs/projects/bsc70/bsc70642/misiones/Series/Models/GAN/",
    "model": {
        "generator": {
            "class": "ai4ha.GAN.Generators.WaveGANGenerator",
            "latent_dim":[512],
            "params": {
                "latent_dim": 512,
                "init_dim": 32,
                "init_channels": 64,
                "output_size": 256,
                "output_channels": 1,
                "kernel_size": 8,
                "stride": 2,
                "normalization": "instance",
                "n_classes": 5
            }
        },
        "discriminator": {
            "class": "ai4ha.GAN.Discriminators.WaveGANDiscriminator",
            "params": {
                "input_size":256,
                "input_channels":1,
                "n_layers":3,
                "init_channels":8,
                "kernel_size":4,
                "stride":2,
                "shift_factor":8,
                "n_classes":5,
                "sp_norm":true
            }
        }
    },
    "optimizer": {
        "lr": 0.0002,
        "beta1": 0.5,
        "beta2": 0.999
    },
    "scheduler": {
        "step_size": 30,
        "gamma": 0.1
    },
    "train": {
        "batch_size": 64,
        "num_epochs": 500,
        "log_interval": 10,
        "save_interval": 10,
        "lr_warmup_steps": 10,
        "lambda_gp": 1,
        "checkpoint_epoch_freq":10,
        "best_model": true
    },
    "data": {
        "path": "data/processed/"
    },
    "ema": {
        "inv_gamma": 1.0,
        "power": 0.75,
        "max_decay": 0.9999
    },
    "projectconf": {
        "total_limit": 2
    },
    "accelerator": {
        "gradient_accumulation_steps": 1,
        "mixed_precision": "no",
        "log_with": "wandb"
    },
    "samples": {
        "samples_freq": 25,
        "samples_num": 20,
        "samples_gen": 10000
    },
    "dataset": {
        "name": "EBHI",
        "nclasses": 5,
        "train": {
            "class": "ai4ha.data.series.MITBIHDataLoader.MITBIHtrain",
            "params": {
                "filename": "/gpfs/projects/bsc70/bsc70642/Data/MITBIH/mitbih_train.csv",
                "n_samples": 2000,
                "resamp": false,
                "oneD": true,
                "fixsize": 256,
                "normalize": false
            }
        },
        "test": {
            "class": "ai4ha.data.series.MITBIHDataLoader.MITBIHtest",
            "params": {
                "filename": "/gpfs/projects/bsc70/bsc70642/Data/MITBIH/mitbih_test.csv",
                "n_samples": 100,
                "resamp": false,
                "oneD": true,
                "fixsize": 256,
                "normalize": false
            }
        },
        "dataloader": {
            "batch_size": 128,
            "num_workers": 6,
            "shuffle": true
        }
    },
    "time": 12
}