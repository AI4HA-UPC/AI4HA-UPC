{
    "name": "Pulse2Pulse-MITBIH-G-NL4-IC32-K16-S2-IN-D-NL4-IC16-K16-S2-SF0-SN-GP05",
    "exp_dir": "/misiones/Series/Models/GAN/",
    "model": {
        "generator": {
            "class": "ai4ha.GAN.Generators.Pulse2PulseGANGenerator",
            "latent_dim": [1, 256], 
            "params": {
                "seq_length": 256,
                "seq_channels": 1,
                "init_channels": 32,
                "n_layers": 4,
                "kernel_size": 16,
                "stride": 2,
                "normalization": "instance",
                "n_classes": 5
            }
  
        },
        "discriminator": {
            "class": "ai4ha.GAN.Discriminators.WaveGANDiscriminator",
            "params": {
                "input_size":256,
                "input_channels":1,
                "n_layers":3,
                "init_channels":16,
                "kernel_size":16,
                "stride":2,
                "shift_factor":0,
                "n_classes":5,
                "sp_norm":true
            }
        }
    },
    "optimizer": {
        "lr": 0.0002,
        "beta1": 0.5,
        "beta2": 0.999
    },
    "scheduler": {
        "step_size": 30,
        "gamma": 0.1
    },
    "train": {
        "batch_size": 64,
        "num_epochs": 1000,
        "log_interval": 10,
        "save_interval": 10,
        "lr_warmup_steps": 10,
        "lambda_gp": 0.5,
        "checkpoint_epoch_freq":10
    },
    "projectconf": {
        "total_limit": 2
    },
    "accelerator": {
        "gradient_accumulation_steps": 1,
        "mixed_precision": "no",
        "log_with": "wandb"
    },
    "samples": {
        "samples_freq": 25,
        "samples_num": 20,
        "samples_gen": 10000
    },
    "dataset": {
        "name": "EBHI",
        "nclasses": 5,
        "train": {
            "class": "ai4ha.data.series.MITBIHDataLoader.MITBIHtrain",
            "params": {
                "filename": "/MITBIH/mitbih_train.csv",
                "n_samples": 2000,
                "resamp": false,
                "oneD": true,
                "fixsize": 256,
                "normalize": false
            }
        },
        "test": {
            "class": "ai4ha.data.series.MITBIHDataLoader.MITBIHtest",
            "params": {
                "filename": "/MITBIH/mitbih_test.csv",
                "n_samples": 100,
                "resamp": false,
                "oneD": true,
                "fixsize": 256,
                "normalize": false
            }
        },
        "dataloader": {
            "batch_size": 16,
            "num_workers": 6,
            "shuffle": true
        }
    },
    "time": 12
}