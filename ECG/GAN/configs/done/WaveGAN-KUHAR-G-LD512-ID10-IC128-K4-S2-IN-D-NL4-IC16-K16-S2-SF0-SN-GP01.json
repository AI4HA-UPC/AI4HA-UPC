{
    "name": "WaveGAN-KUHAR-G-LD512-ID10-IC128-K4-S2-IN-D-NL4-IC16-K16-S2-SF0-SN-GP01",
    "exp_dir": "/misiones/Series/Models/GAN/",
    "model": {
        "generator": {
            "class": "ai4ha.GAN.Generators.WaveGANGenerator",
            "latent_dim":[512],
            "params": {
                "latent_dim": 512,
                "init_dim": 10,
                "init_channels": 128,
                "output_size": 320,
                "output_channels": 6,
                "kernel_size": 4,
                "stride": 2,
                "normalization": "instance",
                "n_classes": 18
            }
        },
        "discriminator": {
            "class": "ai4ha.GAN.Discriminators.WaveGANDiscriminator",
            "params": {
                "input_size":320,
                "input_channels":6,
                "n_layers":4,
                "init_channels":16,
                "kernel_size":16,
                "stride":2,
                "shift_factor":0,
                "n_classes":18,
                "sp_norm":true
            }
        }
    },
    "optimizer": {
        "lr": 0.0002,
        "beta1": 0.5,
        "beta2": 0.999
    },
    "scheduler": {
        "step_size": 30,
        "gamma": 0.1
    },
    "train": {
        "batch_size": 64,
        "num_epochs": 100000,
        "log_interval": 10,
        "save_interval": 10,
        "lr_warmup_steps": 10,
        "lambda_gp": 0.1,
        "checkpoint_epoch_freq":10,
        "best_model": true
    },
    "data": {
        "path": "data/processed/"
    },
    "ema": {
        "inv_gamma": 1.0,
        "power": 0.75,
        "max_decay": 0.9999
    },
    "projectconf": {
        "total_limit": 2
    },
    "accelerator": {
        "gradient_accumulation_steps": 1,
        "mixed_precision": "no",
        "log_with": "wandb"
    },
    "samples": {
        "samples_freq": 25,
        "samples_num": 20,
        "samples_gen": 10000
    },
    "dataset": {
        "name": "KUHAR",
        "nclasses":18,
        "train": {
            "class":"ai4ha.data.series.KUHARDataLoader.KUHARtrain",
            "params":{
                "filename": "/KU-HAR/KU-HAR.csv",
                "channels": 6,
                "padding":20
            }
        },

        "test":{ 
            "class": "ai4ha.data.series.KUHARDataLoader.KUHARtrain",
            "params":{
                "filename": "/KU-HAR/KU-HAR.csv",
                "channels": 6,
                "padding":20
            }
        },
        "dataloader": {
            "batch_size": 128,
            "num_workers": 6,
            "shuffle": true
        }
    },
    "time": 12
}